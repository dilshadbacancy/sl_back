        {
          "name": "Send OTP",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/auth/send-otp",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "auth",
                "send-otp"
              ]
            },
            "description": "Send OTP to user mobile number",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"mobile\": \"9876543210\",\n  \"role\": \"customer\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            }
          }
        },
        {
          "name": "Verify OTP",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/auth/verify-otp",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "auth",
                "verify-otp"
              ]
            },
            "description": "Verify OTP and get access token",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"code\": \"123456\",\n  \"mobile\": \"9876543210\",\n  \"user_id\": \"123e4567-e89b-12d3-a456-426614174000\"\n}",
              "options": {
